

<div class="mainCont">
    <h2><b></b>Hello here are all the users</b></h2>
    
   
    
<% @users.each do |user| %>


    <span class="itemhols">
    
   <h1>User Id: <%= user.id %></h1>
   <h1>email: <%= user.email %></h1>
   <h1>Admin: <%= user.admin %></h1>
    <p><a href="/upgrade/<%= user.id %>" class="button">Make Admin</a></p>
    <p><a href="/downgrade/<%= user.id %>" class="button">Remove Admin</a></p>
    </span>
   
<% end %>
    
    
</div>